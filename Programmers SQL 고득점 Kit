#ORDER BY 오름차순 정렬(디폴트) : ASC
select *
from animal_ins
order by animal_id ASC

#ORDER BY 내림차순 정렬 : DESC
SELECT *
FROM ANIMAL_INS
ORDER BY ANIMAL_ID DESC

#Where로 조건에 맞는 데이터만 출력 : 보호 시작시 상태가 'Sick'인 경우만 출력 -> ==(x) =(o) 
SELECT ANIMAL_ID, NAME
FROM ANIMAL_INS
WEHRE INTAKE_CONDITION = 'Sick'
ORDER BY ANIMAL_ID

